{
	"ReactNativeIDE.panelLocation": "side-panel",
	"window.titleBarStyle": "native",
	"window.customTitleBarVisibility": "never",
	"apc.electron": {
		"frame": false,
		"titleBarStyle": "hidden",
		"trafficLightPosition": {
			"x": 7,
			"y": 5
		}
	},
	"apc.header": {
		"height": 36
	},
	"apc.imports": [
		"/Users/rychillie/Developer/custom-vscode/custom.css"
	],
	"apc.listRow": {
		"height": 24
	},
	"apc.menubar.compact": true,
	"apc.sidebar.titlebar": {
		"height": 32
	},
	"apc.stylesheet": {
		// ".monaco-scrollable-element.mac>.split-view-container>.split-view-view:nth-child(3)": "top:0!important;height:100%!important",
		"body, html": "background-color:#000!important",
		".composite-bar": "width: 100%",
		".composite-bar-container": "width: 100%",
		".composite-bar>.monaco-action-bar": "width: 100%",
		".composite-bar>.monaco-action-bar>.actions-container": "width: 100%; justify-content: space-around",
		".composite.has-composite-bar.header-or-footer.footer": "padding: 0px 12px 0px 8px!important",
		".editor-actions": "display: none",
		".monaco-list-row": "border-radius: 4px;",
		".monaco-list-row.selected": "backgroundColor: transparent;",
		".monaco-workbench .monaco-list:not(.element-focused):focus:before": "display: none;",
		".no-activity-bar.nosidebar:not(.sidebar-right):not(.fullscreen) .inline-tabs-placeholder": "padding-left: 0px!important",
		".nosidebar .inline-tabs-placeholder": "width: 75px",
		".pane-body": "margin: 0 -4px 0 -8px",
		".pane-header": "padding: 0 8px",
		".split-view-container:second-child.visible": "",
		".split-view-view:first-child .pane-header": "display: none !important;",
		".split-view-view:first-child .pane-header[aria-label=['Commits Section']": "display: block !important",
		".title-label > h2": "display: none"
	},
	"breadcrumbs.enabled": false,
	"cSpell.enabled": false,
	"cursor.chat.smoothStreaming": true,
	"cursor.cmdk.useThemedDiffBackground": true,
	"cursor.cpp.disabledLanguages": [
		"plaintext",
		"markdown",
		"scminput"
	],
	"diffEditor.ignoreTrimWhitespace": false,
	"diffEditor.wordWrap": "on",
	"editor.accessibilitySupport": "off",
	"editor.bracketPairColorization.enabled": false,
	"editor.codeActionsOnSave": {
		"source.fixAll": "always",
		"source.sortImports": "always",
		"source.fixAll.eslint": "always",
		"source.organizeImports": "always"
	},
	"editor.formatOnSave": true,
	"editor.cursorBlinking": "smooth",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.fontFamily": "'Cascadia Code', Fira Code, Consolas, Courier New, SFMono-Medium, monospace",
	"editor.fontLigatures": true,
	"editor.fontSize": 16,
	"editor.fontWeight": "500",
	"editor.glyphMargin": false,
	"editor.guides.bracketPairs": true,
	"editor.guides.bracketPairsHorizontal": false,
	"editor.guides.highlightActiveBracketPair": true,
	"editor.guides.highlightActiveIndentation": "always",
	"editor.guides.indentation": true,
	"editor.hideCursorInOverviewRuler": true,
	"editor.inlayHints.enabled": "off",
	"editor.lineNumbers": "on",
	"editor.minimap.maxColumn": 160,
	"editor.minimap.renderCharacters": false,
	"editor.minimap.showSlider": "always",
	"editor.occurrencesHighlight": "off",
	"editor.overviewRulerBorder": false,
	"editor.padding.top": 20,
	"editor.peekWidgetDefaultFocus": "editor",
	"editor.renderLineHighlight": "gutter",
	"editor.renderWhitespace": "none",
	"editor.rulers": [
		80,
		120
	],
	"editor.scrollbar.horizontal": "hidden",
	"editor.scrollbar.horizontalScrollbarSize": 0,
	"editor.scrollbar.ignoreHorizontalScrollbarInContentHeight": true,
	"editor.scrollbar.vertical": "hidden",
	"editor.scrollbar.verticalScrollbarSize": 0,
	"editor.semanticHighlighting.enabled": false,
	"editor.showFoldingControls": "never",
	"editor.smoothScrolling": true,
	"editor.snippetSuggestions": "top",
	"editor.stickyScroll.enabled": true,
	"editor.suggest.insertMode": "replace",
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"editor.tabSize": 2,
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"editor.wordBreak": "normal",
	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 80,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"typescript": "typescriptreact"
	},
	"eslint.alwaysShowStatus": true,
	"eslint.format.enable": true,
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"graphql"
	],
	"explorer.compactFolders": false,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.expand": false,
	"explorer.fileNesting.patterns": {
		".env": ".env*, localhost.pem, localhost-key.pem",
		".env.local": ".env*, localhost.pem, localhost-key.pem",
		"package.json": ".eslint*, prettier*, tsconfig*, vite*, pnpm-*, bun.lockb, nest*, package-lock*, tailwind.config*, postcss.config, eslint*, drizzle.config*, .npmrc"
	},
	"files.autoSave": "onFocusChange",
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.decorations.enabled": true,
	"git.enableSmartCommit": true,
	"git.ignoreRebaseWarning": true,
	"github.copilot.editor.enableAutoCompletions": true,
	"github.copilot.enable": {
		"*": true,
		"markdown": false,
		"plaintext": false,
		"scminput": false
	},
	"gitlens.codeLens.enabled": true,
	"gitlens.currentLine.enabled": false,
	"gitlens.defaultDateLocale": null,
	"gitlens.hovers.currentLine.over": "line",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"notebook.formatOnSave.enabled": true,
	"notebook.stickyScroll.enabled": true,
	"oneDarkPro.editorTheme": "Panda",
	"prettier.arrowParens": "avoid",
	"prettier.jsxSingleQuote": true,
	"prettier.printWidth": 120,
	"prettier.semi": false,
	"prettier.singleQuote": true,
	"prettier.useTabs": true,
	"prisma.showPrismaDataPlatformNotification": false,
	"problems.defaultViewMode": "table",
	"scm.diffDecorations": "gutter",
	"scm.diffDecorationsGutterAction": "none",
	"scm.diffDecorationsGutterVisibility": "hover",
	"scm.diffDecorationsGutterWidth": 1,
	"scm.diffDecorationsIgnoreTrimWhitespace": "true",
	"search.exclude": {
		"**/bower_components": true,
		"**/env": true,
		"**/node_modules": true,
		"**/venv": true
	},
	"security.workspace.trust.untrustedFiles": "open",
	"symbols.hidesExplorerArrows": false,
	"tailwindCSS.emmetCompletions": true,
	"tailwindCSS.experimental.classRegex": [
		[
			"cva\\(([^)]*)\\)",
			"[\"'`]([^\"'`]*).*?[\"'`]"
		],
		[
			"cx\\(([^)]*)\\)",
			"(?:'|\"|`)([^']*)(?:'|\"|`)"
		],
		[
			"clsx\\(([^)]*)\\)",
			"(?:'|\"|`)([^']*)(?:'|\"|`)"
		]
	],
	"telemetry.telemetryLevel": "off",
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1",
		"Q_NEW_SESSION": "1"
	},
	"terminal.integrated.fontSize": 14,
	"terminal.integrated.fontWeight": "500",
	"terminal.integrated.showExitAlert": false,
	"terminal.integrated.stickyScroll.enabled": true,
	"thunder-client.editorFontSize": 16,
	"thunder-client.requestLayout": "Top/Bottom",
	"typescript.preferences.preferTypeOnlyAutoImports": true,
	"typescript.preferences.quoteStyle": "single",
	"typescript.suggest.autoImports": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"update.mode": "default",
	"wakatime.apiKey": "YOUR-WAKATIME-API",
	"window.autoDetectColorScheme": true,
	"window.commandCenter": false,
	"window.confirmBeforeClose": "keyboardOnly",
	"window.menuBarVisibility": "toggle",
	"window.nativeTabs": true,
	"window.newWindowDimensions": "maximized",
	"window.title": "${folderName}",
	"window.zoomLevel": 0.75,
	"workbench.activityBar.location": "bottom",
	"workbench.activityBar.orientation": "vertical",
	"workbench.colorTheme": "One Dark Pro",
	"workbench.editor.enablePreview": false,
	"workbench.editor.labelFormat": "short",
	"workbench.iconTheme": "symbols",
	"workbench.layoutControl.enabled": false,
	"workbench.preferredDarkColorTheme": "One Dark Pro",
	"workbench.preferredLightColorTheme": "Catppuccin Latte",
	"workbench.productIconTheme": "fluent-icons",
	"workbench.statusBar.visible": false,
	"workbench.tree.enableStickyScroll": true,
	"workbench.tree.renderIndentGuides": "none",
	"lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
	"lldb.launch.expressions": "native",
	"editor.minimap.enabled": true,
	"liveSassCompile.settings.formats": [
		// This is Default.
		{
			"format": "expanded",
			"extensionName": ".css",
			"savePath": null
		},
		// You can add more
		{
			"format": "compressed",
			"extensionName": ".min.css",
			"savePath": "/Public/css"
		}
	]
}